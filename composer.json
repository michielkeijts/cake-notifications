{
	"name": "michielkeijts/cake-notifications",
	"type": "cakephp-plugin",
	"description": "CakePHP plugin to send notifications ",
	"keywords": ["cakephp","rest","api","dataobject"],
	"homepage": "http://github.com/michielkeijts/cakephp-rest-api-parser",
	"license": "MIT",
	"authors": [
		{
			"name": "Michiel Keijts",
			"homepage": "http://www.michielkeijts.nl",
			"role": "Contributor"
		}
	],
	"require": {
		"php": ">=7.1",
		"cakephp/cakephp": ">=3.7",
        "symfony/yaml": ">=5.1"
	},
	"require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
	},
	"suggest": {
    	"dereuromark/cakephp-queue": "a generic asynchronous script execution (deferred execution) for CakePHP"
	},
	"support": {
		"source": "https://github.com/michielkeijts/cakephp-minify"
	},
	"autoload": {
		"psr-4": {
			"CakeNotifications\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
		}
	},
	"scripts": {
	}
}
